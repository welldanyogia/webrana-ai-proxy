# =============================================================================
# Webrana AI Proxy - Development Environment Variables
# Copy to .env.dev and fill in values
# =============================================================================

# PostgreSQL (local dev)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=webrana

# Encryption key (32 bytes base64) - Generate with: openssl rand -base64 32
# WARNING: Use different key for production!
MASTER_ENCRYPTION_KEY=

# JWT Secret (64 bytes base64) - Generate with: openssl rand -base64 64
# WARNING: Use different secret for production!
JWT_SECRET=

# Logging level
RUST_LOG=debug
