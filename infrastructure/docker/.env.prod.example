# =============================================================================
# Webrana AI Proxy - Production Environment Variables
# Copy to .env and fill in actual values
# =============================================================================

# PostgreSQL
POSTGRES_USER=webrana
POSTGRES_PASSWORD=CHANGE_ME_GENERATE_SECURE_PASSWORD
POSTGRES_DB=webrana

# Redis (generate with: openssl rand -base64 24)
REDIS_PASSWORD=CHANGE_ME_GENERATE_SECURE_PASSWORD

# Encryption (generate with: openssl rand -base64 32)
MASTER_ENCRYPTION_KEY=CHANGE_ME_32_BYTES_BASE64

# JWT Secret (generate with: openssl rand -base64 64)
JWT_SECRET=CHANGE_ME_64_BYTES_BASE64

# Frontend URLs
NEXT_PUBLIC_API_URL=https://api.webrana.id
NEXTAUTH_URL=https://webrana.id
NEXTAUTH_SECRET=CHANGE_ME_32_BYTES_BASE64

# Email (Resend)
RESEND_API_KEY=re_xxxxxxxxxxxx

# Payment (Midtrans) - Use sandbox keys for staging
MIDTRANS_SERVER_KEY=SB-Mid-server-xxxx
MIDTRANS_CLIENT_KEY=SB-Mid-client-xxxx
MIDTRANS_IS_SANDBOX=true

# Docker image tag
TAG=latest

# Logging level
RUST_LOG=info
